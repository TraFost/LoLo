{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [{ "handle": "filesystem" }, { "src": "/(.*)", "dest": "/" }],
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build:client",
  "outputDirectory": "client/dist"
}
